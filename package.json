{
  "name": "rob-myers.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn next-cfg && concurrently 'next -p 3002' 'yarn watch-svg-meta'",
    "build": "yarn next-cfg && next build",
    "analyze": "ANALYZE=true yarn build",
    "export": "next export",
    "start": "yarn next-cfg && next start",
    "next-cfg": "tsc -p scripts",
    "types": "tsc --noEmit --incremental true",
    "postinstall": "patch-package",
    "ts-script": "ts-node -O '{ \"module\": \"commonjs\", \"isolatedModules\": false }'",
    "rename-pngs": "yarn ts-script media/scripts/rename-pngs",
    "minify-pngs": "yarn ts-script media/scripts/minify-pngs",
    "simplify-pngs": "yarn ts-script media/scripts/simplify-pngs",
    "render-layout": "yarn ts-script media/scripts/render-layout",
    "svg-meta": "yarn ts-script media/scripts/svg-meta",
    "watch-svg-meta": "nodemon --delay 2s --config media/scripts/svg-meta.nodemon.json --exec 'yarn svg-meta'",
    "https-proxy": "node_modules/local-ssl-proxy/bin/local-ssl-proxy --source 443 --target 3002",
    "local-deploy": "yarn build && yarn export",
    "deploy-from-local": "touch out/.nojekyll && cd scripts && ts-node gh-pages.ts",
    "deploy": "yarn local-deploy && yarn deploy-from-local"
  },
  "dependencies": {
    "braces": "^3.0.2",
    "classnames": "^2.3.1",
    "cli-columns": "^3.1.2",
    "codemirror": "^5.62.0",
    "debounce": "^1.2.1",
    "earcut": "^2.2.3",
    "flexlayout-react": "^0.5.12",
    "getopts": "^2.3.0",
    "goober": "^2.0.41",
    "json-stringify-pretty-compact": "^3.0.0",
    "lodash.clonedeep": "^4.5.0",
    "mvdan-sh": "^0.5.0",
    "nanoid": "^3.1.23",
    "next": "^11.1.0",
    "next-images": "^1.7.0",
    "next-plugin-preact": "^3.0.6",
    "poly2tri": "^1.5.0",
    "polygon-clipping": "^0.15.3",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "react": "npm:@preact/compat@0.0.4",
    "react-beforeunload": "^2.4.0",
    "react-dom": "npm:@preact/compat@0.0.4",
    "react-intersection-observer": "^8.32.0",
    "react-markdown": "^6.0.2",
    "react-query": "^3.18.1",
    "react-reverse-portal": "^2.1.0",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "react-use-measure": "^2.0.4",
    "rehype-raw": "^5.1.0",
    "remark-gfm": "^1.0.0",
    "rxjs": "7.0.0-beta.10",
    "safe-json-stringify": "^1.2.0",
    "safe-stable-stringify": "^1.1.1",
    "svg-path-parser": "^1.1.0",
    "xterm": "^4.10.0",
    "xterm-addon-fit": "^0.5.0",
    "zustand": "^3.4.1"
  },
  "devDependencies": {
    "@types/braces": "^3.0.0",
    "@types/codemirror": "^5.60.2",
    "@types/debounce": "^1.2.1",
    "@types/earcut": "^2.1.1",
    "@types/gh-pages": "^3.2.0",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.2",
    "@types/react": "^17.0.2",
    "@types/react-beforeunload": "^2.1.0",
    "@types/safe-json-stringify": "^1.1.2",
    "@types/svg-path-parser": "^1.1.2",
    "@types/webpack": "^4.41.26",
    "@types/webpack-bundle-analyzer": "^3.9.1",
    "@types/webpack-env": "^1.16.0",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpackbar": "^4.0.0",
    "canvas": "^2.8.0",
    "cheerio": "^1.0.0-rc.10",
    "concurrently": "^6.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "local-ssl-proxy": "^1.3.0",
    "nodemon": "^2.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "patch-package": "^6.4.7",
    "raw-loader": "^4.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-merge": "^5.7.3",
    "webpackbar": "^5.0.0-3"
  }
}
