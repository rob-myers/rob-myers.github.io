  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/generator": "^7.10.2",
    "cdt2d": "^1.0.0",
    "classnames": "^2.2.6",
    "es-react": "16.12.0",
    "localforage": "^1.7.3",
    "monaco-editor": "^0.20.0",
    "monaco-typescript": "^3.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-reverse-portal": "^2.0.0",
    "rectangle-decomposition": "^1.0.0",
    "redux": "4.0.5",
    "redux-observable": "^1.2.0",
    "redux-persist": "^6.0.0",
    "remote-redux-devtools": "^0.5.16",
    "rxjs": "^6.5.4",
    "sass.js": "^0.11.1",
    "scss-parser": "^1.0.4",
    "shortid": "^2.2.15",
    "spark-md5": "^3.0.1",
    "ts-morph": "^7.1.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.2.0"
  },
  "scripts": {
    "dev": "yarn next-cfg && concurrently 'next -p 3001' 'yarn watch-packages-manifest'",
    "build": "yarn next-cfg && yarn build-packages-manifest && next build",
    "start": "yarn next-cfg && next start",
    "export": "next export",
    "deploy": "yarn local-deploy && yarn deploy-from-local",
    "local-deploy": "rm -rf .next && yarn build && yarn export",
    "deploy-from-local": "touch out/.nojekyll && yarn gh-pages",
    "next-cfg": "tsc -p scripts && yarn ensure-public-js",
    "gh-pages": "cd scripts && ts-node gh-pages.ts",
    "test": "jest --watch --verbose",
    "check-types": "tsc --noEmit",
    "syntax-remotedev": "remotedev --hostname=localhost --port=3002 --wsEngine=ws",
    "analyze-build": "rm -rf .next && yarn next-cfg && ANALYZE=true next build",
    "ensure-public-js": "yarn cp-sass-worker && yarn cp-es-react",
    "cp-sass-worker": "cp node_modules/sass.js/dist/sass.worker.js public",
    "cp-es-react": "mkdir -p public/es-react && cp node_modules/es-react/dev/*.js public/es-react",
    "ts-node": "node -r esm -r  ts-node/register",
    "react-refresh-test": "yarn ts-node model/dev-env/react-refresh-transform",
    "build-packages-manifest": "yarn ts-node scripts/packages-manifest.ts",
    "watch-packages-manifest": "nodemon --watch 'public/package/**/*' -e ts,tsx,scss --exec 'yarn build-packages-manifest'"
  },
  "devDependencies": {
    "@mdx-js/loader": "^1.6.6",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@types/braces": "^3.0.0",
    "@types/classnames": "^2.2.10",
    "@types/gh-pages": "2.0.1",
    "@types/glob": "^7.1.1",
    "@types/node": "13.7.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "16.9.19",
    "@types/react-redux": "7.1.7",
    "@types/redux-mock-store": "1.0.2",
    "@types/remote-redux-devtools": "^0.5.4",
    "@types/scss-parser": "^1.0.0",
    "@types/shortid": "^0.0.29",
    "@types/webpack": "4.41.3",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-env": "^1.15.1",
    "@types/webpack-merge": "4.1.5",
    "@types/worker-plugin": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "babel-jest": "25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-mdx": "^1.7.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-standard": "4.0.1",
    "esm": "^3.2.25",
    "extract-css-chunks-webpack-plugin": "^4.7.4",
    "file-loader": "^6.0.0",
    "gh-pages": "2.2.0",
    "identity-obj-proxy": "3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "25.1.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "next": "^9.4.1",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.1",
    "react-test-renderer": "16.12.0",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "remotedev-server": "^0.3.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svg-inline-loader": "^0.8.2",
    "ts-node": "8.6.2",
    "typescript": "3.7.5",
    "url-loader": "^4.1.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-merge": "4.2.2",
    "worker-loader": "^2.0.0"
  },
  "sideEffects": [
    "*.css"
  ]
